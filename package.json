{
  "name": "@nice-digital/niceorg-client",
  "version": "2.0.0",
  "description": "Client side assets wwww.nice.org.uk using the NICE Design System",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack serve --config webpack.dev.js",
    "dev:local": "npm run dev -- --env niceorgBaseUrl=http://niceorg",
    "dev:dev": "npm run dev -- --env niceorgBaseUrl=https://dev.nice.org.uk",
    "dev:test": "npm run dev -- --env niceorgBaseUrl=https://test.nice.org.uk",
    "dev:alpha": "npm run dev -- --env niceorgBaseUrl=https://alpha.nice.org.uk",
    "build": "webpack --config webpack.prod.js",
    "// TESTS": "",
    "test": "jest",
    "// LINTING/CODE STYLE": "",
    "prettier": "prettier --check .",
    "prettier:fix": "npm run prettier -- --write",
    "lint": "npm run prettier && npm run lint:js && npm run lint:scss",
    "lint:js": "eslint .",
    "lint:js:fix": "npm run lint -- --fix",
    "lint:scss": "stylelint \"**/*.scss\"",
    "css": "sass src/scss/app.scss --load-path=node_modules"
  },
  "author": "NICE Digital (https://www.nice.org.uk/digital)",
  "contributors": [
    "NOC contributors (https://github.com/nice-digital/niceorg-client/graphs/contributors)",
    "Ian Routledge (https://ediblecode.com)"
  ],
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@nice-digital/browserslist-config": "^2.0.1",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "cheerio": "^1.0.0-rc.12",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.24.1",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-webpack-plugin": "^3.2.0",
    "express": "^4.18.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "mini-css-extract-plugin": "^2.6.1",
    "needle": "^3.1.0",
    "postcss": "^8.4.14",
    "postcss-discard-duplicates": "^5.1.0",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "webpack": "^5.85.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@nice-digital/design-system": "^5.0.13-alpha.0",
    "jquery": "^3.6.0",
    "preact": "^10.10.0"
  },
  "volta": {
    "node": "14.20.0"
  }
}
