{
  "name": "@nice-digital/niceorg-client",
  "version": "2.0.0",
  "description": "Client side assets wwww.nice.org.uk using the NICE Design System",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack serve --config webpack.dev.js",
    "dev:local": "npm run dev -- --env niceorgBaseUrl=http://niceorg",
    "dev:dev": "npm run dev -- --env niceorgBaseUrl=https://dev.nice.org.uk",
    "dev:test": "npm run dev -- --env niceorgBaseUrl=https://test.nice.org.uk",
    "dev:alpha": "npm run dev -- --env niceorgBaseUrl=https://alpha.nice.org.uk",
    "build": "webpack --config webpack.prod.js",
    "// TESTS": "",
    "test": "jest",
    "// LINTING/CODE STYLE": "",
    "prettier": "prettier --check .",
    "prettier:fix": "npm run prettier -- --write",
    "lint": "npm run prettier && npm run lint:js && npm run lint:scss",
    "lint:js": "eslint .",
    "lint:js:fix": "npm run lint -- --fix",
    "lint:scss": "stylelint \"**/*.scss\""
  },
  "author": "NICE Digital (https://www.nice.org.uk/digital)",
  "contributors": [
    "NOC contributors (https://github.com/nice-digital/niceorg-client/graphs/contributors)",
    "Ian Routledge (https://ediblecode.com)"
  ],
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@nice-digital/browserslist-config": "^2.0.1",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "cheerio": "^1.0.0-rc.6",
    "copy-webpack-plugin": "^8.1.0",
    "core-js": "^3.10.1",
    "css-loader": "^5.2.0",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-webpack-plugin": "^2.5.3",
    "express": "^4.16.3",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.9",
    "needle": "^2.6.0",
    "postcss": "^8.3.0",
    "postcss-discard-duplicates": "^5.0.0",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "slugify": "1.4.7",
    "stylelint": "^13.12.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-prettier": "^1.2.0",
    "webpack": "^5.28.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@nice-digital/design-system": "^1.2.30",
    "jquery": "^3.6.0",
    "preact": "^10.5.13"
  }
}
